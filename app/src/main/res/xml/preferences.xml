<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="@layout/activity_main">
    <PreferenceCategory android:title="앱 정보">

    <Preference
        android:key="keyword_version"
        android:title="앱 버전"
        android:summary="1.1.1"
        android:text="엡 버전"/>

        <Preference
            android:key="key_notice"
            android:text="공지사항"
            android:title="공지사항" />

        <Preference
            android:key="key_rule"
            android:text="커뮤니티 이용규칙"
            android:title="커뮤니티 이용규칙" />
        <ListPreference
            android:defaultValue="1"
            android:entries="@array/ringtone_alignment"
            android:entryValues="@array/ringtone_alignment"
            android:key="list_preference_1"
            android:title="알림설정"
            android:text="알림 설정"/>


    </PreferenceCategory>

    <PreferenceCategory android:title="계정 설정">
        <Preference
            android:key="key_logout"
            android:text="로그아웃"
            android:title="로그아웃" />
    </PreferenceCategory>

</PreferenceScreen>